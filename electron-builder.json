{
  "productName": "posetronic",
  "appId": "com.orionark.posetronic",
  "directories": {
    "output": "release/"
  },
  "files": [
      "**/*",
      "!**/*.ts",
      "!*.code-workspace",
      "!LICENSE.md",
      "!package.json",
      "!package-lock.json",
      "!angular.webpack.js",
      "!electron-builder-trial.json",
      "!electron-builder.json",
      "!src/",
      "!e2e/",
      "!hooks/",
      "!angular.json",
      "!_config.yml",
      "!karma.conf.js",
      "!tsconfig.json",
      "!tslint.json",
      "!releases/",
      "!release/",
      "!.angular/",
      "!.angular",
      "!.npmrc",
      "!main.js.map",
      "!tsconfig-serve.json",
      "!trial/"
  ],
  "win": {
    "icon": "dist/assets/icons",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist/assets/icons",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/assets/icons",
    "target": [
      "AppImage"
    ]
  }
}
